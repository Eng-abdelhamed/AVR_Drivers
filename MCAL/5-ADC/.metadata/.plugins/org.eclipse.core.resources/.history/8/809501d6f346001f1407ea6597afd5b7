#ifndef ADC_PRIVATE_H
#define ADC_PRIVATE_H

#define DIV_BY_2   0
#define DIV_BY_4   1
#define DIV_BY_8   3
#define DIV_BY_16  4
#define DIV_BY_32  5
#define DIV_BY_64  6
#define DIV_BY_128 7


#define PRESCALAR_MASK 0b11111000




